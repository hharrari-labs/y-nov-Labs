{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Utilisateur</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nomutilisateur</th>
                <td>{{ entity.nomutilisateur }}</td>
            </tr>
            <tr>
                <th>Membres</th>
                <td>{{ entity.membres }}</td>
            </tr>
            <tr>
                <th>Goupe</th>
                <td>{{ entity.goupe }}</td>
            </tr>
            <tr>
                <th>Annee</th>
                <td>{{ entity.annee|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ entity.password }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ entity.adresse }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('utilisateur') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('utilisateur_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
