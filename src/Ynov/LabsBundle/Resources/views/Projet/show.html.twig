{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Projet</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nomprojet</th>
                <td>{{ entity.nomprojet }}</td>
            </tr>
            <tr>
                <th>Statutprojet</th>
                <td>{{ entity.statutprojet }}</td>
            </tr>
            <tr>
                <th>Videoprojet</th>
                <td>{{ entity.videoprojet }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Datecreation</th>
                <td>{{ entity.datecreation|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Datemajprojet</th>
                <td>{{ entity.datemajprojet|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Technologie</th>
                <td>{{ entity.technologie }}</td>
            </tr>
            <tr>
                <th>Equipe</th>
                <td>{{ entity.equipe }}</td>
            </tr>
            <tr>
                <th>Lienprojet</th>
                <td>{{ entity.lienprojet }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ entity.logo }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('projet') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('projet_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
